<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Aniverse.Common.mapper.UserInfo.Setting_Mapper">

<select id="search_private" parameterType="String" resultType="boolean">
select COALESCE(private, false) from usersetting  where userid =#{userid};
</select>
<insert id="Initsetting" parameterType="java.util.HashMap">
insert into usersetting(userid,private_date)values(#{userid} ,#{date});
</insert>
</mapper>